   "source": [
    "cache = [[-1] * 30 for x in range(30)]\n",
    "def bino(n, r):\n",
    "    if r == 0 or n == r:\n",
    "        return 1\n",
    "    if cache[n][r] != -1:\n",
    "        return cache[n][r]\n",
    "    cache[n][r] = bino(n-1,r-1) + bino(n-1,r)\n",
    "    return cache[n][r]\n",
    "    "
   ]
