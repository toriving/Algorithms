    "def pick(n, picked, toPick):\n",
    "    if toPick == 0:\n",
    "        print(picked)\n",
    "\n",
    "    smallest = 0 if len(picked) == 0 else picked[-1] + 1\n",
    "    \n",
    "    for i in range(smallest, n):\n",
    "        picked.append(i)\n",
    "        pick(n, picked, toPick - 1)\n",
    "        picked.pop()\n",
    "    "
