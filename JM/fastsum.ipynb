    "def fastSum(n):\n",
    "    if n == 1:\n",
    "        return 1\n",
    "    if n % 2 == 1:\n",
    "        return fastSum(n-1) + n\n",
    "    return 2*fastSum(n/2) + (n/2)*(n/2)\n"
   ]
  },
