    "def solve(N):\n",
    "    INF = 987654321\n",
    "    cnt = 0\n",
    "    for i in range(INF):\n",
    "        if str(i).count('666'):\n",
    "            cnt += 1\n",
    "        if N == cnt:\n",
    "            return i"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "if __name__ == \"__main__\" :\n",
    "    N = int(input())\n",
    "    \n",
    "    print(solve(N))"
